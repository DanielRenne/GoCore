bash $GOPATH/src/github.com/DanielRenne/goCoreAppTemplate/bin/kill_servers
go install github.com/DanielRenne/goCoreAppTemplate
cd $GOPATH/src/github.com/DanielRenne/goCoreAppTemplate/web/app/javascript
bash build.sh
cd $GOPATH
go build src/github.com/DanielRenne/goCoreAppTemplate/goCoreAppTemplate.go
cd $GOPATH
whoami=$(whoami)
if [[ "$whoami" == "root" ]]; then
    goCoreAppTemplate
else
    sudo goCoreAppTemplate
fi
