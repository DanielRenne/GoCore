whoami=$(whoami)
if [[ "$whoami" == "root" ]]; then
    pkill compile
    ps -ax | grep goWatch | awk '{print $1}' | xargs kill > /dev/null 2>&1
else
    sudo pkill compile
    ps -ax | grep goWatch | awk '{print $1}' | xargs sudo kill > /dev/null 2>&1
fi
bash $GOPATH/src/github.com/DanielRenne/goCoreAppTemplate/bin/kill_servers