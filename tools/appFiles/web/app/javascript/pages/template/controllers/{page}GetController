package controllers

import (
	"github.com/DanielRenne/goCoreAppTemplate/viewModel"
	"github.com/DanielRenne/goCoreAppTemplate/sessionFunctions"
)


func (self *-CAPCAMEL-Controller) Root(context session_functions.RequestContext, uriParams map[string]string, respond session_functions.ServerResponse) {

	var vm viewModel.-CAPCAMEL-ViewModel
	vm.LoadDefaultState()
	respond(PARAM_REDIRECT_NONE, PARAM_SNACKBAR_MESSAGE_NONE, SNACKBAR_TYPE_SUCCESS, nil, PARAM_TRANSACTION_ID_NONE, vm)

}
